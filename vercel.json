{
  "version": 2,
  "builds": [
    {
      "src": "frontend/meeting-summarizer/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/meeting-summarizer/dist/$1"
    }
  ],
  "buildCommand": "cd frontend/meeting-summarizer && npm run build",
  "outputDirectory": "frontend/meeting-summarizer/dist",
  "installCommand": "cd frontend/meeting-summarizer && npm install"
}